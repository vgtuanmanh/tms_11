<tr>
  <th>
    <%= course.id %> &nbsp;
  </th>
  <th>
    <%= link_to course.name, admin_course_path(course) %> &nbsp;
  </th>
  <th>
    <%= course.description %> &nbsp;
  </th>
  <th>
    <%= link_to "Edit", "#", class: "btn btn-primary" %> &nbsp;
  </th>
  <th>
    <%= link_to "Assign Members", new_admin_course_assignment_path(course),
                                  class: "btn btn-primary" %> &nbsp;
  </th>
  <th>
    <% if current_user.admin? && !current_user?(course) %>
      <%= link_to "Delete", "#", class: "btn btn-primary",
      classmethod: :delete, data: { confirm: "You sure?" } %> &nbsp;
    <% end %>
  <th>
</tr>